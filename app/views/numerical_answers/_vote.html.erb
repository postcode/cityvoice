<tr>
  <td rowspan="2"><%= link_to(location.name, location) %></td>
  <td rowspan="2"><%= question.question_text %></td>
  <td>Yes</td>
  <td>
    <%= render partial: 'numerical_answers/vote_meter', locals: { number: question.response_counts(location)[1], percentage: question.response_percentages(location)[1] } %>
  </td>
</tr>
<tr>
  <td>No</td>
  <td>
    <%= render partial: 'numerical_answers/vote_meter', locals: { number: question.response_counts(location)[2], percentage: question.response_percentages(location)[2] } %>
  </td>
</tr>
